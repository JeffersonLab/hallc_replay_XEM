TH1F pgtr_xtar 'SHMS Golden Track X_{tar}; X_{tar} (cm); Number of Entries / 0.05 cm' P.gtr.x 80 -2 2 pcut_CleanTrack
TH1F pgtr_ytar 'SHMS Golden Track Y_{tar}; Y_{tar} (cm); Number of Entries / 0.25 cm' P.gtr.y 60 -7.5 7.5 pcut_CleanTrack
TH1F pgtr_yptar 'SHMS Golden Track Y'_{tar}; Y'_{tar} (rad); Number of Entries / 0.002 rad' P.gtr.ph 100 -.1 .1 pcut_CleanTrack
TH1F pgtr_xptar 'SHMS Golden Track X'_{tar}; X'_{tar} (rad); Number of Entries / 0.002 rad' P.gtr.th 100 -.1 .1 pcut_CleanTrack

TH2F pgtr_xtar_vs_reactz 'SHMS Golden Track X_{tar}; Z_{react} / 0.1 cm; X_{tar} / 0.05 cm' P.react.z P.gtr.x 200 -10 10 80 -2 2 pcut_CleanTrack

TH2F pgtr_xtar_vs_ytar   'SHMS Golden Track X_{tar} vs Y_{tar}; Y_{tar} (cm) / 0.25 cm; X_{tar} (cm) / 0.25 cm' P.gtr.y P.gtr.x 80 -20 20 60 -7.5 7.5 pcut_CleanTrack
TH2F pgtr_xptar_vs_yptar 'SHMS Golden Track X'_{tar} vs Y'_{tar}; Y'_{tar} / 0.002 rad; X'_{tar} / 0.002 rad' P.gtr.ph P.gtr.th 100 -.1 .1 100 -.1 .1 pcut_CleanTrack
TH2F pgtr_xtar_vs_yptar  'SHMS Golden Track X_{tar} vs Y'_{tar}; Y'_{tar} / 0.002 rad; X_{tar} / 0.25 cm'  P.gtr.ph P.gtr.x 100 -.1 .1 60 -7.5 7.5 pcut_CleanTrack
TH2F pgtr_xtar_vs_xptar  'SHMS Golden Track X_{tar} vs X'_{tar}; X'_{tar} / 0.002 rad; X_{tar} / 0.25 cm' P.gtr.th P.gtr.x 100 -.1 .1 60 -7.5 7.5 pcut_CleanTrack
TH2F pgtr_ytar_vs_yptar  'SHMS Golden Track Y_{tar} vs Y'_{tar}; Y'_{tar} / 0.002 rad; Y_{tar} / 0.25 cm'  P.gtr.ph P.gtr.y 100 -.1 .1 60 -7.5 7.5 pcut_CleanTrack
TH2F pgtr_ytar_vs_xptar  'SHMS Golden Track Y_{tar} vs X'_{tar}; X'_{tar} / 0.002 rad; Y_{tar} / 0.25 cm' P.gtr.th P.gtr.y 100 -.1 .1 60 -7.5 7.5 pcut_CleanTrack
